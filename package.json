{
  "name": "creatella-react-components",
  "version": "0.1.0",
  "description": "Createllas React Components Library",
  "author": "SchwSimon",
  "license": "MIT",
  "repository": "SchwSimon/creatella-react-components",
  "dependencies": {},
  "sideEffects": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-lib": "rimraf ./lib/ && babel ./src/lib --out-dir ./lib --copy-files",
    "commit-push": "git add * && git commit -m . && git push origin master",
    "pub": "yarn build-lib && yarn commit-push"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-react": "^7.6.3",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.2.0",
    "rimraf": "^3.0.0"
  },
  "files": [
    "lib"
  ]
}
